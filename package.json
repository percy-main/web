{
  "name": "website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "dotenv -- astro",
    "circular:check": "dpdm ./src/actions/index.ts ",
    "contentful:types": "set -a && source .env && set +a && cf-content-types-generator -s $CDN_SPACE_ID -e $CDN_ENVIRONMENT -t $CDN_CMA_TOKEN -o src/__generated__ -Xrg",
    "contentful:create-merge": "set -a && source .env && set +a && contentful-merge create --space $CDN_SPACE_ID --source dev --target master --cda-token $CDN_TOKEN --output-file ./.contentful/merge.json",
    "contentful:apply-merge": "set -a && source .env && set +a && contentful-merge apply --space $CDN_SPACE_ID --environment master --cma-token $CDN_CMA_TOKEN --file ./.contentful/merge.json",
    "email": "email dev",
    "format": "prettier . --write",
    "lint": "eslint src/**",
    "email:dev": "email dev",
    "auth:migration": "CLI=true zx ./scripts/auth-migrate.mjs",
    "db:types": "DATABASE_URL=./local.db kysely-codegen --out-file ./src/lib/db/__generated__/db.ts",
    "db:migration": "zx ./scripts/create-migration.mjs",
    "db:up": "CLI=true set -a && source .env && set +a && tsx ./scripts/migrate-up",
    "postinstall": "patch-package",
    "test": "vitest run",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/netlify": "^6.3.1",
    "@astrojs/react": "^4.2.5",
    "@astrojs/sitemap": "^3.3.1",
    "@better-auth/passkey": "^1.4.19",
    "@contentful/live-preview": "^4.6.3",
    "@contentful/rich-text-html-renderer": "^17.0.0",
    "@contentful/rich-text-react-renderer": "^16.0.1",
    "@libsql/client": "^0.14.0",
    "@libsql/kysely-libsql": "^0.4.1",
    "@netlify/functions": "^5.1.2",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tabs": "^1.1.2",
    "@react-email/components": "^0.0.36",
    "@react-email/render": "1.0.4",
    "@stripe/react-stripe-js": "^5.6.0",
    "@stripe/stripe-js": "^5.4.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@tanstack/react-query": "^5.64.1",
    "@vis.gl/react-google-maps": "^1.5.0",
    "add-to-calendar-button-react": "^2.13.8",
    "astro": "^5.17.3",
    "astro-breadcrumbs": "^3.3.1",
    "astro-font": "^0.1.81",
    "better-auth": "^1.4.19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "contentful": "^11.3.4",
    "contentful-management": "^11.40.3",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "html-to-text": "^9.0.5",
    "input-otp": "^1.4.2",
    "kysely": "^0.28.11",
    "lodash": "^4.17.23",
    "lucide-react": "^0.474.0",
    "mailgun.js": "^11.1.0",
    "postcss": "^8.4.49",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "react-qr-code": "^2.0.15",
    "schema-dts": "^1.1.2",
    "stripe": "^17.5.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^4.0.0",
    "tailwindcss-animate": "^1.0.7",
    "ts-pattern": "^5.6.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.4.19",
    "@eslint/js": "^9.19.0",
    "@playwright/test": "^1.58.2",
    "@tailwindcss/postcss": "^4.0.0",
    "@tailwindcss/vite": "^4.0.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/eslint": "^9.6.1",
    "@types/html-to-text": "^9.0.4",
    "@types/json-patch": "^0.0.33",
    "@types/lodash": "^4.17.13",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/parser": "^8.22.0",
    "autoprefixer": "^10.4.20",
    "cf-content-types-generator": "^2.15.5",
    "contentful-merge": "^2.6.5",
    "dotenv-cli": "^8.0.0",
    "dpdm": "^3.14.0",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "json-patch": "^0.7.0",
    "kysely-codegen": "^0.20.0",
    "patch-package": "^8.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-compiler-runtime": "^19.0.0-beta-b2e8e9c-20241220",
    "react-email": "3.0.6",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.22.0",
    "vitest": "^3.0.5",
    "zx": "^8.3.0"
  },
  "overrides": {
    "kysely-codegen": {
      "@libsql/kysely-libsql": "^0.4.1"
    }
  },
  "doxla": {
    "components": {
      "dir": "docs/components"
    },
    "dependencies": {}
  }
}
